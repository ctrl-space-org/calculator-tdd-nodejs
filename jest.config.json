{
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": -10
    },
    "./src/domain/": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    },
    "./server.js": {
      "branches": 40,
      "functions": 40
    }
  },
  "verbose": false,
  "collectCoverage": true,
  "collectCoverageFrom": [
    "TDD Calculator",
    "**/*.{js,}",
    "!**/node_modules/**",
    "!**/docs/**",
    "!**/docs-src/**",
    "!**/coverage/**"
  ],
  "coverageReporters": [
    "html",
    "lcov",
    "text",
    "clover",
    "json"
  ],
  "coverageDirectory": "coverage",
  "projects": [
    {
      "name": "TDD Calculator",
      "displayName": {
        "name": "TDD Calculator - jest",
        "color": "blueBright"
      }
    },
    {
      "displayName": {
        "name": "TDD Calculator - lint",
        "color": "yellowBright"
      },
      "name": "TDD Calculator - lint",
      "runner": "jest-runner-eslint",
      "testMatch": [
        "**/src/**/*.{js,}",
        "!**/node_modules/**",
        "!**/docs/**",
        "!**/coverage/**"
      ]
    }
  ]
}